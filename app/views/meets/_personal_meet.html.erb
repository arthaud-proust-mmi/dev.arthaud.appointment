<div class="col-sm-4 mb-4">
  <div class="card" id="<%= dom_id meet %>">
    <div class="card-body">
      <h5 class="card-title">
        <%= meet.planned_at %>
      </h5>
      <h6 class="card-subtitle mb-2 text-muted">
        <%= link_to meet.service.title, service_path(meet.service_id) %>
      </h6>
      <%= link_to "Voir", meet, class:'btn btn-primary' %>
    </div>
  </div>
</div>